{
  "expo": {
    "name": "TAZIRI",
    "slug": "taziri2025",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo-removebg-preview_p22obg.png",
    "splash": {
      "image": "./assets/images/logo-removebg-preview_p22obg.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6B35"
    },
    "scheme": "tazirinext",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mohadoudou.tazirinext"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-removebg-preview_p22obg.png",
        "backgroundColor": "#FF6B35",
        "monochromeImage": "./assets/images/logo-removebg-preview_p22obg.png"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET"
      ],
      "package": "com.mohadoudou.tazirinext",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-removebg-preview_p22obg.png",
      "build": {
        "babel": {
          "include": []
        }
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-file-system",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo-removebg-preview_p22obg.png",
          "color": "#FF6B35",
          "sounds": [
            "./assets/sounds/notification.wav"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8aedaf22-cd67-409d-b52a-ba739d9c60a9"
      }
    },
    "owner": "mohadoudou"
  }
}
